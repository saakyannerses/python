arr = [5, 10, 123, 24, 502, 100, 18, 999, 40]

# Вывод двузначных элементов
print("Двузначные элементы массива:")
for i in arr:
    if 10 <= i < 100:
        print(i, end=" ")

# Вывод трехзначных элементов
print("\nТрехзначные элементы массива:")
for i in arr:
    if 100 <= i < 1000:
        print(i, end=" ")
